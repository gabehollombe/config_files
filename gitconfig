[color]
	diff = auto
	status = auto
	branch = auto
	ui = auto
[core]
	excludesfile = ~/.gitignore
[alias]
  c = commit -m
	ci = commit
  a = add
  di = diff
  dic = diff --cached
  pl = pull
  ps = push
  plre = pull --rebase
  st = status
	co = checkout
  out = log origin..HEAD
	br = branch
  lg = log -p

[branch]
	autosetupmerge = true

[push]
	default = current
